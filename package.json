{
  "scripts": {
    "client:dev": "webpack --config config/webpack/client.js",
    "client:prod": "cross-env NODE_ENV=production npm run client:dev",
    "client:start": "webpack-dev-server --config config/webpack/client.js",
    "server:dev": "webpack --config config/webpack/server.js",
    "server:prod": "cross-env NODE_ENV=production npm run server:dev",
    "server:start": "node dist/server.js",
    "build:dev": "npm run client:dev && npm run server:dev",
    "build:prod": "npm run client:prod && npm run server:prod",
    "start:dev": "npm-run-all --parallel server:dev client:start",
    "start:prod": "npm run build:prod && npm run server:start"
  },
